class Player {
  float health;
  PlayerBody body;

  Player(PlayerBody playerBody) {
    health = 100f;
    body = playerBody;

    po.subscribe(body, POCodes.Key.W);
    po.subscribe(body, POCodes.Key.A);
    po.subscribe(body, POCodes.Key.S);
    po.subscribe(body, POCodes.Key.D);
    po.subscribe(body, POCodes.Key.UP);
    po.subscribe(body, POCodes.Key.DOWN);
    po.subscribe(body, POCodes.Key.LEFT);
    po.subscribe(body, POCodes.Key.RIGHT);
  }

  void update () {
    if (body.getPosition().x > 940 &&
          body.getPosition().y > 400 &&
          body.getPosition().y < 600) {
      println("FINISH"); 
    }
  }
}

